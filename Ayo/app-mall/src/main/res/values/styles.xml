<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">


    <style name="lib_style_base">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="lib_style_text" parent="lib_style_base">
        <item name="android:textColor">@color/lib_color_font2</item>
    </style>
    <style name="lib_style_text_size2" parent="lib_style_text">
        <item name="android:textSize">17sp</item>
    </style>
    <style name="lib_style_shop_return_title" parent="lib_style_text_size2">
        <item name="android:textColor">@color/lib_color_font2</item>
        <item name="android:textSize">16sp</item>
    </style>


    <style name="mall_center_order_item_image">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_marginLeft">8dp</item>
    </style>

    <style name="mall_center_order_item_badge">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">15dp</item>
        <item name="android:layout_gravity">right|top</item>
        <item name="android:textSize">10sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="mall_center_order_item_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/lib_color_font3</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>

    <style name="share_item_image">
        <item name="android:layout_width">@dimen/share_icon</item>
        <item name="android:layout_height">@dimen/share_icon</item>
    </style>

    <style name="invoice_form_lable_text">
        <item name="android:textSize">@dimen/lib_font_size4</item>
        <item name="android:textColor">@color/lib_color_font2</item>
        <item name="android:gravity">center_vertical|left</item>
    </style>

    <style name="invoice_form_edit_text">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:textSize">@dimen/lib_font_size4</item>
        <item name="android:textColor">@color/lib_color_font2</item>
        <item name="android:textColorHint">@color/lib_color_font4</item>
        <item name="android:background">@null</item>
        <item name="android:maxLines">1</item>
    </style>

    <!--购物车-->
    <style name="shopping_car_item_top">
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:textColor">@color/goods_name</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="shopping_car_checkbox">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:button">@drawable/selector_shoppingcar_checkbox</item>
        <item name="android:checked">true</item>
        <item name="android:focusable">false</item>
    </style>

    <style name="coupon_item_checkbox">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:button">@drawable/selector_coupon_item_checkbox</item>
        <item name="android:checked">true</item>
        <item name="android:focusable">false</item>
    </style>

    <style name="shopping_car_price">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@color/goods_price</item>
        <item name="android:text">@string/goods_price</item>
    </style>


    <style name="add_address_edit">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/address_value</item>
        <item name="android:textSize">@dimen/address_text</item>
        <item name="android:background">@null</item>
        <item name="android:gravity">center|left</item>
        <item name="android:paddingTop">@dimen/address_margin</item>
        <item name="android:paddingBottom">@dimen/address_margin</item>
    </style>

    <style name="order_detail_button_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/selector_green_border_white_bg</item>
        <item name="android:paddingBottom">7dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">7dp</item>
        <item name="android:textColor">@color/title</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>

    <style name="order_detail_relativelayout_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingBottom">@dimen/confirmorder_pay_margin</item>
        <item name="android:paddingLeft">@dimen/confirm_order_margin</item>
        <item name="android:paddingRight">@dimen/pay_listitem_margin_right</item>
        <item name="android:paddingTop">@dimen/confirmorder_pay_margin</item>
    </style>

    <style name="mall_home_item_price_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ff3939</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
    </style>


    <style name="goods_comment_type_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">@dimen/goods_comment_inner_margin</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@drawable/selector_product_final_comment_text_color</item>
        <item name="android:textSize">@dimen/lib_font_size4</item>
    </style>

    <style name="goods_comment_type_container_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <!--<item name="android:background">@drawable/selector_product_final_comment_bg</item>-->
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
    </style>


    <style name="Style_TextViewDefault">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="Style_Share_Group_TextView" parent="Style_TextViewDefault">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">middle</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="Style_TextViewListEmpty_View" parent="Style_TextViewDefault">
        <item name="android:paddingRight">30dp</item>
        <item name="android:paddingLeft">30dp</item>
        <item name="android:textColor">#7a7a7a</item>
        <item name="android:textSize">16sp</item>
    </style>


    <style name="Style_TextViewXListViewFoot" parent="Style_TextViewDefault">
        <item name="android:textColor">@color/lib_color_font4</item>
        <item name="android:textSize">@dimen/lib_font_size4</item>
    </style>


    <style name="xlistview">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:divider">@color/mall_listview_divider</item>
        <item name="android:dividerHeight">0.5dp</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:textSize">17sp</item>
        <item name="android:fastScrollEnabled">false</item>
        <item name="android:scrollbarSize">4dip</item>
        <item name="android:scrollbarThumbVertical">@drawable/kit_scrollbar_vertical_thumb</item>
        <item name="android:scrollbarTrackVertical">@drawable/kit_scrollbar_vertical_track</item>
    </style>

    <!--<style name="goods_detail_style" parent="AppTheme">
        <item name="vpiCirclePageIndicatorStyle">@style/CustomCirclePageIndicator</item>
    </style>-->



    <style name="AppTheme.Toolbar" parent="Widget.AppCompat.ActionBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:background">?attr/colorPrimary</item>
        <item name="android:theme">@style/ThemeOverlay.AppCompat.Dark.ActionBar</item>
    </style>

    <style name="pn_CustomLabelBlack">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:background">@drawable/pn_stroke_black</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingStart">@dimen/six</item>
        <item name="android:paddingEnd">@dimen/six</item>
        <item name="android:paddingTop">@dimen/zero</item>
        <item name="android:paddingBottom">@dimen/one</item>
        <item name="android:layout_marginEnd">@dimen/eight</item>
        <item name="android:textSize">@dimen/font_11</item>
        <item name="android:textColor">@color/mainBlack</item>
    </style>
    <style name="pn_CustomLabelBlueBg">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:background">@color/mainBlue</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingStart">@dimen/four</item>
        <item name="android:paddingBottom">@dimen/one</item>
        <item name="android:paddingEnd">@dimen/four</item>
        <item name="android:layout_marginEnd">@dimen/eight</item>
        <item name="android:textSize">@dimen/font_11</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/mainWhite</item>
    </style>

    <style name="pn_CustomLabelBlueGenderStroke">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginEnd">@dimen/eight</item>
        <item name="android:background">@drawable/pn_stroke_blue</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:drawablePadding">@dimen/five</item>
        <item name="android:paddingStart">@dimen/six</item>
        <item name="android:paddingEnd">@dimen/six</item>
        <item name="android:paddingTop">@dimen/zero</item>
        <item name="android:paddingBottom">@dimen/one</item>
        <item name="android:textSize">@dimen/font_11</item>
        <item name="android:textColor">@color/mainBlue</item>
    </style>


    <style name="pn_LineOneVertical">
        <item name="android:layout_width">@dimen/one</item>
        <item name="android:background">@color/mainBlack5B</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="pn_LineEight">
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/mainGrayF0</item>
        <item name="android:layout_height">@dimen/eight</item>
    </style>

    <style name="pn_LineOnePointFive">
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/mainGrayF0</item>
        <item name="android:layout_height">@dimen/one_point_five</item>
    </style>

    <style name="pn_UserCenterIntegralTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">@dimen/nine</item>
        <item name="android:textColor">@color/mainBlack</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/font_12</item>
    </style>

    <style name="pn_UserCenterIntegralNumTextView">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart" tools:ignore="NewApi">@dimen/sixteen</item>
        <item name="android:textColor">@color/mainRed</item>
        <item name="android:textSize">@dimen/font_14</item>
    </style>
    <!--User center label-->
    <style name="pn_UserCenterLabel">
        <item name="android:background">@drawable/pn_shape_gray_3</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginEnd">@dimen/eight</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingBottom">@dimen/four</item>
        <item name="android:paddingTop">@dimen/four</item>
        <item name="android:paddingStart">@dimen/eight</item>
        <item name="android:paddingEnd">@dimen/eight</item>
        <item name="android:textColor">@color/mainBlack</item>
        <item name="android:textSize">@dimen/font_12</item>
    </style>

    <style name="pn_UserTitleAvatar">
        <item name="android:layout_width">@dimen/thirty_two</item>
        <item name="android:layout_height">@dimen/thirty_two</item>
        <item name="android:layout_marginEnd">@dimen/five</item>
    </style>
    <style name="pn_QuestionTitleTextView">
        <item name="android:layout_marginTop">@dimen/nineteen</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">@dimen/forty_eight</item>
        <item name="android:layout_weight">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:lineSpacingExtra">@dimen/eight</item>
        <item name="android:maxLines">2</item>
        <item name="android:textSize">@dimen/font_17</item>
        <item name="android:textColor">@color/mainBlack</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="pn_QuestionAnswerTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/mainGrayC6</item>
        <item name="android:textSize">@dimen/font_12</item>
    </style>

    <style name="pn_QuestionDescriptionTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:lineSpacingExtra">@dimen/ten</item>
        <item name="android:maxLines">2</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/twenty_one</item>
        <item name="android:textSize">@dimen/font_14</item>
        <item name="android:textColor">@color/mainGrayC6</item>
    </style>

    <style name="pn_AnswerSingleTextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/font_14</item>
        <item name="android:textColor">@color/mainGrayC6</item>
    </style>
    <style name="pn_AnswerTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">@dimen/nine</item>
        <item name="android:background">@color/mainBlack</item>
        <item name="android:padding">@dimen/two</item>
        <item name="android:text">@string/answer</item>
        <item name="android:textColor">@color/mainWhite</item>
    </style>

    <style name="pn_AnswerNoneTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/mainGray88</item>
        <item name="android:textSize">@dimen/font_14</item>
        <item name="android:text">@string/no_answer_get_first_answer</item>
    </style>
</resources>