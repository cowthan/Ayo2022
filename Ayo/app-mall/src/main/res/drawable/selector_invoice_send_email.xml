<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <stroke android:width="0dp" android:color="#999999" />
            <solid android:color="#999999" />
            <corners android:radius="2dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="0dp" android:color="@color/orange2_pressed" />
            <solid android:color="@color/orange2_pressed" />
            <corners android:radius="2dp" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <stroke android:width="0dp" android:color="@color/orange2" />
            <solid android:color="@color/orange2" />
            <corners android:radius="2dp" />
        </shape>
    </item>

</selector>