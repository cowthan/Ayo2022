<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/list_selector_normal" />
            <stroke android:width="1dp" android:color="@color/list_selector_normal" />
            <corners android:radius="2dp" />
        </shape>
    </item>
    <item android:drawable="@drawable/icon_goods_selected" android:state_checked="true" />
    <item android:state_checked="false">
        <shape>
            <corners android:radius="2dp" />
            <solid android:color="#f5f5f5" />
        </shape>

    </item>
</selector>