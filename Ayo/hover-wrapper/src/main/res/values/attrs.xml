<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LogcatTextView">
        <attr name="verboseColor" format="color"/>
        <attr name="debugColor" format="color"/>
        <attr name="errorColor" format="color"/>
        <attr name="infoColor" format="color"/>
        <attr name="warningColor" format="color"/>
        <attr name="consoleColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="CurveChartView">
        <attr name="ccv_XTextPadding" format="integer"/>
        <attr name="ccv_YTextPadding" format="integer"/>
        <attr name="ccv_MaxValueMulti" format="float"/>
        <attr name="ccv_MinValueMulti" format="float"/>
        <attr name="ccv_YPartCount" format="integer"/>
        <attr name="ccv_DataSize" format="integer"/>
        <!--<attr name="ccv_YUnitName" format="string"/>-->
        <attr name="ccv_YFormat" format="string"/>
        <attr name="ccv_XYColor" format="color"/>
        <attr name="ccv_XYStrokeWidth" format="float"/>
        <attr name="ccv_LineColor" format="color"/>
        <attr name="ccv_LineStrokeWidth" format="float"/>
        <attr name="ccv_FillColor" format="color"/>
        <attr name="ccv_YLabelColor" format="color"/>
        <attr name="ccv_YLabelSize" format="float"/>
        <attr name="ccv_GraduatedLineColor" format="color"/>
        <attr name="ccv_GraduatedLineStrokeWidth" format="float"/>
    </declare-styleable>
</resources>