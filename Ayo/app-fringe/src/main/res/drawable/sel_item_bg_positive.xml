<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#88a0b3cd" />
            <stroke
                android:width="0.0dp"
                android:color="#ffffff" />
            <corners
                android:radius="10dp"/>
        </shape>
    </item>

    <item>
        <shape>
            <solid android:color="#88B0C4DE" />
            <stroke
                android:width="0.0dp"
                android:color="#FFFFF0" />
            <corners
                android:radius="10dp"/>
        </shape>
    </item>

</selector>


