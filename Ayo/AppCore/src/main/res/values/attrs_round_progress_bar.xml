<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RoundProgressBar">
        <attr name="rpb_roundColor" format="color"/>
        <attr name="rpb_roundProgressColor" format="color"/>
        <attr name="rpb_roundWidth" format="dimension"></attr>
        <attr name="rpb_textColor" format="color" />
        <attr name="rpb_textSize" format="dimension" />
        <attr name="rpb_max" format="integer"></attr>
        <attr name="rpb_textIsDisplayable" format="boolean"></attr>
        <attr name="rpb_style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>

</resources>
